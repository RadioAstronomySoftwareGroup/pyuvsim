[run]
concurrency = thread, multiprocessing
parallel = true
omit =
    */tests/*
    */docs/*
    setup.py
source =
    pyuvsim
patch =
    subprocess

[report]
omit =
    */tests/*
    */docs/*
    setup.py

show_missing = true

[paths]
source =
    src
    */site-packages"
tests =
    tests
    */tests
